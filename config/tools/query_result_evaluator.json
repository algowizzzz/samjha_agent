{
  "name": "query_result_evaluator",
  "implementation": "external.tools.query_result_evaluator.QueryResultEvaluatorTool",
  "description": "Evaluate query results against original intent (LLM or heuristics)",
  "version": "1.0.0",
  "enabled": true,
  "inputSchema": {
    "type": "object",
    "properties": {
      "original_query": { "type": "string" },
      "execution_result": { "type": "object" },
      "execution_stats": { "type": "object" },
      "table_schema": { "type": "object" }
    },
    "required": ["original_query", "execution_result"]
  }
}


